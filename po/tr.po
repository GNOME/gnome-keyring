#, c-format
msgid "waiting for lock (held by %d%s) %s...\n"
msgstr "kilit bekleniyor (%d%s tarafından oluşturulan) %s...\n"

#: egg/dotlock.c:1085
msgid "(deadlock?) "
msgstr "(çıkmaza mı girdi?)"

