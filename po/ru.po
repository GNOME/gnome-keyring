msgid "(deadlock?) "
msgstr "(взаимная блокировка?) "

#: egg/dotlock.c:1124
#, c-format
msgid "lock `%s' not made: %s\n"
msgstr "блокировка «%s» не выполнена: %s\n"

#: egg/dotlock.c:1150
#, c-format
msgid "waiting for lock %s...\n"
msgstr "ожидание блокировки %s…\n"

